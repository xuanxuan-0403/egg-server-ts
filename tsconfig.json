{
    "extends": "@eggjs/tsconfig",
    "compilerOptions": {
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "declaration": false,
        "paths": {
            "@/module/*": ["app/module/*"],
            "utils/*": ["utils/*"]
        },
        "baseUrl": ".",
        "importHelpers": true,
        "strict": true
    }
}
